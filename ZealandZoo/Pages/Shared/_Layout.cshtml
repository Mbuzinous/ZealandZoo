    </header>


        

    
    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="footer">
        <section class="gridContainerFooterThreeColumns">
            <div class="grid-item-sm"></div>
            <div class="grid-item-sm">
                <a href="https://www.facebook.com/cafezealand/" target="_blank"><img src="~/css/facebook_145802.png" alt="Facebook Icon" /></a>
                <a class="grid-item-zealand" href="https://www.zealand.dk/" target="_blank"> <img src="~/css/Zealand_logo_black_RGB_1600px_72dpi.png" alt="Zealand Logo" /> </a>
                <a href="https://www.instagram.com/cafezealandzoo?igsh=bWJrYTF6bXdoYmx5" target="_blank"><img src="~/css/instagram_174855.png" alt="Instagram Icon" /></a>
